@model Team
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}


<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Photo">Photo</label>
        <input asp-for="Photo" class="form-control"/>
        <span asp-validation-for="Photo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Fullname">Full Name</label>
        <input asp-for="Fullname" class="form-control" />
        <span asp-validation-for="Fullname" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Position">Position</label>
        <input asp-for="Position" class="form-control" />
        <span asp-validation-for="Position" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Facelogo">Facebook Logo</label>
        <input asp-for="Facelogo" class="form-control" />
        <span asp-validation-for="Facelogo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Twitlogo">Twitter Logo</label>
        <input asp-for="Twitlogo" class="form-control" />
        <span asp-validation-for="Twitlogo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Googlogo">Google Logo</label>
        <input asp-for="Googlogo" class="form-control" />
        <span asp-validation-for="Googlogo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Linkelogo">Linkedin Logo</label>
        <input asp-for="Linkelogo" class="form-control" />
        <span asp-validation-for="Linkelogo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Otherlogo">Other Logo</label>
        <input asp-for="Otherlogo" class="form-control" />
        <span asp-validation-for="Otherlogo" class="text-danger"></span>
    </div>
    <input type="submit" class="btn btn-primary btn-block btn-lg" value="Submit" />
</form>

@section scripts
{
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.1/jquery.validate.min.js"></script>  
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js"></script>
}